163 159                 case 1: G1=2; break;  
164 160                 case 2: G2=2; break;  
165 161                 case 3: G3=pow(10,2); break;  
166 162                 case 4: G4=2; break;  
167 163             }  
168 164         }  
169 165         else if(strcmp(WARNA, "orange")==0)  
170 166         {  
171 167             switch(N)  
172 168             {  
173 169                 case 1: G1=3; break;  
174 170                 case 2: G2=3; break;  
175 171                 case 3: G3=pow(10,3); break;  
176 172                 case 4: G4=0; break;  
177 173             }  
178 174         }  
