127 124 void hitung_4gelang()  
128 125 {  
129 126     int G1=0, G2=0, G3=0, G4=0;  
130 127     int N=0;  
131 128     char WARNA[20];  
132 129     char p= '%';  
133 130  
134   
135 131     for (N=1; N<=4; N++)  
136 132     {  
137 133         printf(" Masukkan warna GELANG ke %d : ", N);  
138 134         scanf("%s", WARNA);  
139 135         if(strcmp(WARNA, "hitam")==0)  
140 136         {  
141 137             switch(N)  
142 138             {  
143 139                 case 1: G1=0; break;  
