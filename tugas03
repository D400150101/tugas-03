5   
6 5 void hitung_3gelang()  
7 6 {  
8 7     int G1=0, G2=0, G3=0;  
9 8     int N=0;  
10 9     char WARNA[20];  
11 10  
12   
13 11     for (N=1; N<=3; N++)  
14 12     {  
15 13         printf(" Masukkan warna GELANG ke %d : ", N);  
16 14         scanf("%s", WARNA);  
17 15         if(strcmp(WARNA, "hitam")==0)  
18 16         {  
19 17             switch(N)  
20 18             {  
21 19                 case 1: G1=0; break;  
22 20                 case 2: G2=0; break;  
23 21                 case 3: G3=pow(10,0); break;  
24 22             }  
25 23         }  
26 24         else if(strcmp(WARNA, "coklat")==0)  
27 25         {  
28 26             switch(N)  
29 27             {  
30 28                 case 1: G1=1; break;  
31 29                 case 2: G2=1; break;  
32 30                 case 3: G3=pow(10,1); break;  
33 31             }  
34 32         }  
35 33         else if(strcmp(WARNA, "merah")==0)  
36 34         {  
37 35             switch(N)  
38 36             {  
39 37                 case 1: G1=2; break;  
40 38                 case 2: G2=2; break;  
41 39                 case 3: G3=pow(10,2); break;  
42 40             }  
43 41         }  
44 42         else if(strcmp(WARNA, "orange")==0)  
45 43         {  
46 44             switch(N)  
47 45             {  
48 46                 case 1: G1=3; break;  
49 47                 case 2: G2=3; break;  
50 48                 case 3: G3=pow(10,3); break;  
51 49             }  
52 50         }  
53 51         else if(strcmp(WARNA, "kuning")==0)  
54 52         {  
55 53             switch(N)  
56 54             {  
57 55                 case 1: G1=4; break;  
58 56                 case 2: G2=4; break;  
59 57                 case 3: G3=pow(10,4); break;  
60 58             }  
61 59         }  
62 60          else if(strcmp(WARNA, "hijau")==0)  
63 61         {  
64 62             switch(N)  
65 63             {  
66 64                 case 1: G1=5; break;  
67 65                 case 2: G2=5; break;  
68 66                 case 3: G3=pow(10,5); break;  
69 67             }  
70 68         }  
71 69         else if(strcmp(WARNA, "biru")==0)  
72 70         {  
73 71             switch(N)  
74 72             {  
75 73                 case 1: G1=6; break;  
76 74                 case 2: G2=6; break;  
77 75                 case 3: G3=pow(10,6); break;  
78 76             }  
79 77         }  
80 78         else if(strcmp(WARNA, "ungu")==0)  
81 79         {  
82 80             switch(N)  
83 81             {  
84 82                 case 1: G1=7; break;  
85 83                 case 2: G2=7; break;  
86 84                 case 3: G3=pow(10,7); break;  
87 85             }  
88 86         }  
89 87         else if(strcmp(WARNA, "abu-abu")==0)  
90 88         {  
91 89             switch(N)  
92 90             {  
93 91                 case 1: G1= 8; break;  
94 92                 case 2: G2=8; break;  
95 93                 case 3: G3=pow(10,8); break;  
96 94             }  
97 95         }  
98 96         else if(strcmp(WARNA, "putih")==0)  
99 97         {  
100 98             switch(N)  
101 99             {  
102 100                 case 1: G1=9; break;  
103 101                 case 2: G2=9; break;  
104 102                 case 3: G3=pow(10,9); break;  
105 103             }  
106 104         }  
107 105          else if(strcmp(WARNA, "emas")==0)  
108 106         {  
109 107             switch(N)  
110 108             {  
111 109                case 3: G3=pow(10,-1); break;  
112 110             }  
113 111         }  
114 112          else if(strcmp(WARNA, "perak")==0)  
115 113         {  
116 114             switch(N)  
117 115             {  
118 116                case 3: G3=pow(10,-2); break;  
119 117             }  
120 118         }  
121 119     }  
122 120     N=((G1*10)+G2)*G3;  
123 121     printf("\nNilai resistor R = %d ohm", N);  
124 122 }  
