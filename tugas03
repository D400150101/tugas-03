522 514                 case 4: G4=pow(10,7); break;  
523 515                 case 5: G5=0.1; break;  
524 516                 case 6: G6=5; break;  
525 517             }  
526 518         }  
527 519         else if(strcmp(WARNA, "abu-abu")==0)  
528 520         {  
529 521             switch(N)  
530 522             {  
531 523                 case 1: G1=8; break;  
532 524                 case 2: G2=8; break;  
533 525                 case 3: G3=8; break;  
534 526                 case 4: G4=pow(10,8); break;  
535 527                 case 5: G5=0.05; break;  
536 528                 case 6: G6=1; break;  
537 529             }  
538 530         }  
539 531         else if(strcmp(WARNA, "putih")==0)  
540 532         {  
541 533             switch(N)  
542 534             {  
543 535                 case 1: G1=9; break;  
544 536                 case 2: G2=9; break;  
545 537                 case 3: G3=9; break;  
546 538                 case 4: G4=pow(10,9); break;  
547 539                 case 5: G5=0; break;  
548 540                 case 6: G6=0; break;  
549 541             }  
550 542         }  
551 543          else if(strcmp(WARNA, "emas")==0)  
552 544         {  
553 545             switch(N)  
554 546             {  
555 547                case 4: G4=pow(10,-1); break;  
556 548                case 5: G5=5; break;  
557 549             }  
558 550         }  
559 551          else if(strcmp(WARNA, "perak")==0)  
560 552         {  
561 553             switch(N)  
562 554             {  
563 555                case 4: G4=pow(10,-2); break;  
564 556                case 5: G5=10; break;  
565 557             }  
566 558         }  
567 559         else if(strcmp(WARNA, "tak berwarna")==0)  
568 560         {  
569 561             switch(N)  
570 562             {  
571 563                case 5: G5=20; break;  
572 564             }  
573 565         }  
574 566     }  
575 567     N=((((G1*100)+(G2*10))+G3)*G4);  
576 568     printf("\nNilai resistor %d ohm dengan toleransi %d%c %d ppm/K", N,G5,p,G6);  
577 569 }  
578 570  
579   
580 571 int main()  
581 572 {  
582 573     int GELANG = 0;  
583 574     while((GELANG<3)||(GELANG>6))  
584 575     {  
585 576         printf("Masukkan jumlah gelang : ");  
586 577         scanf("%d",&GELANG);  
587 578         if((GELANG<3) || (GELANG>6))  
588 579         {  
589 580             printf("\n===========================");  
590 581             printf("\nNilai GELANG antara 3 s/d 6");  
591 582             printf("/n===========================\n");  
592 583         }  
593 584         if(GELANG==3)  
594 585         {  
595 586             hitung_3gelang();  
596 587         }  
597 588         else if(GELANG==4)  
598 589         {  
599 590             hitung_4gelang();  
600 591         }  
601 592         else if(GELANG==5)  
602 593         {  
603 594             hitung_5gelang();  
604 595         }  
605 596         else if(GELANG==6)  
606 597         {  
607 598             hitung_6gelang();  
608 599         }  
609 600     }  
610 601     return 0;  
611 602 }  
