419 412 void hitung_6gelang()  
420 413 {  
421 414     int G1=0, G2=0, G3=0, G4=0, G5=0, G6=0;  
422 415     int N=0;  
423 416     char WARNA[20];  
424 417     char p= '%';  
425 418  
426   
427 419     for (N=1; N<=6; N++)  
428 420     {  
429 421         printf(" Masukkan warna GELANG ke %d : ", N);  
430 422         scanf("%s", WARNA);  
431 423         if(strcmp(WARNA, "hitam")==0)  
432 424         {  
433 425             switch(N)  
434 426             {  
435 427                 case 1: G1=0; break;  
436 428                 case 2: G2=0; break;  
437 429                 case 3: G3=0; break;  
438 430                 case 4: G4=pow(10,0); break;  
439 431                 case 5: G5=0; break;  
440 432                 case 6: G6=250; break;  
441 433             }  
442 434         }  
443 435         else if(strcmp(WARNA, "coklat")==0)  
444 436         {  
445 437             switch(N)  
446 438             {  
447 439                 case 1: G1=1; break;  
448 440                 case 2: G2=1; break;  
449 441                 case 3: G3=1; break;  
450 442                 case 4: G4=pow(10,1); break;  
451 443                 case 5: G5=1; break;  
452 444                 case 6: G6=100; break;  
453 445             }  
454 446         }  
455 447         else if(strcmp(WARNA, "merah")==0)  
456 448         {  
457 449             switch(N)  
458 450             {  
459 451                 case 1: G1=2; break;  
460 452                 case 2: G2=2; break;  
461 453                 case 3: G3=2; break;  
462 454                 case 4: G4=pow(10,2); break;  
463 455                 case 5: G5=2; break;  
464 456                 case 6: G6=50; break;  
465 457             }  
466 458         }  
467 459         else if(strcmp(WARNA, "orange")==0)  
468 460         {  
469 461             switch(N)  
470 462             {  
471 463                 case 1: G1=3; break;  
472 464                 case 2: G2=3; break;  
473 465                 case 3: G3=3; break;  
474 466                 case 4: G4=pow(10,3); break;  
475 467                 case 5: G5=0; break;  
476 468                 case 6: G6=15; break;  
477 469             }  
478 470         }  
479 471         else if(strcmp(WARNA, "kuning")==0)  
480 472         {  
481 473             switch(N)  
482 474             {  
483 475                 case 1: G1=4; break;  
484 476                 case 2: G2=4; break;  
485 477                 case 3: G3=4; break;  
486 478                 case 4: G4=pow(10,4); break;  
487 479                 case 5: G5=0; break;  
488 480                 case 6: G6=25; break;  
489 481             }  
490 482         }  
491 483          else if(strcmp(WARNA, "hijau")==0)  
492 484         {  
493 485             switch(N)  
