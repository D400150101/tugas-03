181 177             switch(N)  
182 178             {  
183 179                 case 1: G1=4; break;  
184 180                 case 2: G2=4; break;  
185 181                 case 3: G3=pow(10,4); break;  
186 182                 case 4: G4=0; break;  
187 183             }  
188 184         }  
189 185          else if(strcmp(WARNA, "hijau")==0)  
190 186         {  
191 187             switch(N)  
192 188             {  
193 189                 case 1: G1=5; break;  
194 190                 case 2: G2=5; break;  
195 191                 case 3: G3=pow(10,5); break;  
196 192                 case 4: G4=0.5; break;  
197 193             }  
198 194         }  
199 195         else if(strcmp(WARNA, "biru")==0)  
200 196         {  
201 197             switch(N)  
202 198             {  
203 199                 case 1: G1=6; break;  
204 200                 case 2: G2=6; break;  
205 201                 case 3: G3=pow(10,6); break;  
206 202                 case 4: G4=0.25; break;  
207 203             }  
208 204         }  
209 205         else if(strcmp(WARNA, "ungu")==0)  
210 206         {  
211 207             switch(N)  
212 208             {  
213 209                 case 1: G1=7; break;  
214 210                 case 2: G2=7; break;  
215 211                 case 3: G3=pow(10,7); break;  
216 212                 case 4: G4=0.1; break;  
217 213             }  
218 214         }  
219 215         else if(strcmp(WARNA, "abu-abu")==0)  
220 216         {  
221 217             switch(N)  
222 218             {  
223 219                 case 1: G1=8; break;  
224 220                 case 2: G2=8; break;  
225 221                 case 3: G3=pow(10,8); break;  
226 222                 case 4: G4=0.05; break;  
227 223             }  
228 224         }  
229 225         else if(strcmp(WARNA, "putih")==0)  
230 226         {  
231 227             switch(N)  
232 228             {  
233 229                 case 1: G1=9; break;  
234 230                 case 2: G2=9; break;  
235 231                 case 3: G3=pow(10,9); break;  
236 232                 case 4: G4=0; break;  
237 233             }  
238 234         }  
239 235          else if(strcmp(WARNA, "emas")==0)  
240 236         {  
241 237             switch(N)  
242 238             {  
243 239                case 3: G3=pow(10,-1); break;  
244 240                case 4: G4=5; break;  
245 241             }  
246 242         }  
247 243          else if(strcmp(WARNA, "perak")==0)  
248 244         {  
249 245             switch(N)  
250 246             {  
251 247                case 3: G3=pow(10,-2); break;  
252 248                case 4: G4=10; break;  
253 249             }  
254 250         }  
255 251         else if(strcmp(WARNA, "tak berwarna")==0)  
256 252         {  
257 253             switch(N)  
258 254             {  
259 255                case 4: G4=20; break;  
260 256             }  
261 257         }  
262 258     }  
263 259     N=((G1*10)+G2)*G3;  
264 260     printf("\nNilai resistor %d ohm dengan toleransi %d%c", N, G4, p);  
265 261 }  
